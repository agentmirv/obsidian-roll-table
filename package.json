{
	"name": "obsidian-roll-table",
	"version": "1.0.0",
	"description": "Roll on markdown tables in your Obsidian vault",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"copy-files": "cp main.js styles.css manifest.json ../../obsidian/Test/.obsidian/plugins/obsidian-roll-table/",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.2.6",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@dice-roller/rpg-dice-roller": "^5.5.0"
	}
}
